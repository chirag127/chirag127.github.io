# ============================================================
# GITHUB CONFIGURATION
# ============================================================
GH_USERNAME="your_github_username"
GH_TOKEN="your_github_token"

# ============================================================
# GOOGLE GEMINI CONFIGURATION
# ============================================================
# Primary Single Key
GEMINI_API_KEY="your_primary_gemini_key"

# Key Pool (Comma Separated for Rotation)
GEMINI_API_KEYS="key1,key2,key3..."

# Google Gemini API Key (BACKUP - 27B to 1B models)
# Get from: https://aistudio.google.com/apikey
# Free tier: 30 RPM, 14,400 RPD
# Models: gemma-3-27b-it, gemma-3-12b-it, gemma-3-4b-it, gemma-3-1b-it
# GEMINI_API_KEY="your_backup_gemini_key"

# ============================================================
# OTHER LLM PROVIDERS
# ============================================================
# Cerebras
CEREBRAS_API_KEY="your_cerebras_api_key"

# Groq
GROQ_API_KEY="your_groq_api_key"

# Jules
JULES_API_KEY="your_jules_api_key"

# Mistral API Key (OPTIONAL - 24B models)
# Get from: https://console.mistral.ai/api-keys
# Free tier: Experiment plan available
# Models: mistral-small-3.1-24b, open-mistral-nemo
MISTRAL_API_KEY="your_mistral_api_key"

# NVIDIA NIM API Key (OPTIONAL - 70B models)
# Get from: https://build.nvidia.com (generate API key)
# Free tier: 40 RPM, phone verification required
# Models: meta/llama-3.3-70b-instruct
NVIDIA_API_KEY="your_nvidia_api_key"

# Cloudflare Workers AI (OPTIONAL - 8B models)
# Get from: https://dash.cloudflare.com (AI > Workers AI)
# Free tier: 100k requests/day
# Models: @cf/meta/llama-3.1-8b-instruct
CLOUDFLARE_API_TOKEN="your_cloudflare_api_token"
CLOUDFLARE_ACCOUNT_ID="your_cloudflare_account_id"

# ============================================================
# DATASETS & RESEARCH
# ============================================================
# Hugging Face API (Model Cards / Papers)
# Get from: https://huggingface.co/settings/tokens
HF_TOKEN="your_huggingface_token"

# Kaggle API (Datasets / Competitions)
# Get from: https://www.kaggle.com/settings/account
KAGGLE_USERNAME="your_kaggle_username"
KAGGLE_KEY="your_kaggle_key"

# Semantic Scholar API (Research Papers - Higher Rate Limits)
# Request at: https://www.semanticscholar.org/product/api#api-key-form
SEMANTIC_SCHOLAR_API_KEY="your_semantic_scholar_api_key"

# Unpaywall (Open Access PDFs - Just email)
UNPAYWALL_EMAIL="your_email_for_unpaywall"

# ============================================================
# FUTURE TRENDS APIS (ALL OPTIONAL)
# ============================================================
# If not provided, the corresponding source is silently disabled.
# The system works without any of these keys.

# Product Hunt API (Pre-Trend Signals)
# Get from: https://www.producthunt.com/v2/docs
PRODUCT_HUNT_API_KEY="your_product_hunt_api_key"

# NewsAPI.ai (AI/Tech News)
# Get from: https://newsapi.ai
NEWSAPI_AI_KEY="your_newsapi_ai_key"

# ============================================================
# SOURCES THAT REQUIRE NO API KEY (ALWAYS ENABLED)
# ============================================================
# - arXiv (Public API)
# - PapersWithCode (Public API)
# - Hacker News (Public API)
# - Lobsters (Public API)
# - GitHub Trending (Uses GITHUB_TOKEN)
# - TechCrunch RSS (Public Feed)
# - Stack Overflow (Public API)


# ==============================================================================
# DEPLOYMENT ENABLE/DISABLE FLAGS (All disabled for testing safety)
# ==============================================================================
ENABLE_NETLIFY=false
ENABLE_VERCEL=false
ENABLE_CLOUDFLARE=false
ENABLE_SURGE=false
ENABLE_RENDER=false
ENABLE_NEOCITIES=false
ENABLE_FIREBASE=false
ENABLE_DENO=false
ENABLE_AZURE=false
ENABLE_DIGITALOCEAN=false
ENABLE_FLEEK=false
ENABLE_GITLAB=false
ENABLE_CODEBERG=false


# ==============================================================================
# OMNI-PUBLISHER ENVIRONMENT CONFIGURATION
